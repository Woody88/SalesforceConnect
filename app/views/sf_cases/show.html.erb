<!-- <p id="notice"><%= notice %></p> -->
<h2>Case-<%= @sf_case.CaseNumber %></h2>
<dl class="dl-horizontal">
    <dt>Id</dt>
    <dd><%= @sf_case.Id %></dd>
    <dt>Subject</dt>
    <dd><%= @sf_case.Subject %></dd>
    <dt>Status</dt>
    <dd><%= @sf_case.Status %></dd>
    <dt>Origin</dt>
    <dd><%= @sf_case.Origin %></dd>
    <dt>Priority</dt>
    <dd><%= @sf_case.Priority %></dd>

</dl>

<%= link_to edit_sf_case_path(@sf_case.Id), :class => "btn btn-primary" do %>
  <i class="fa fa-pencil"></i>
<% end %>
<%= link_to "/sf_cases/#{@sf_case.Id}", method: :delete, data: { confirm: 'Are you sure?' }, :class => "btn btn-danger" do %>
          <i class="fa fa-trash-o"></i>
<% end %>
<p>
<%= link_to 'Back ', sf_cases_path %>
</p>
